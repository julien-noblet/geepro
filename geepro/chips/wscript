# -*- coding: utf-8 -*-
# chips
def build(bld):
  print('â†’ building chips')
  modules_base=["24Cxx","27xx","28xx","93Cxx","mcs48","mcs51","prom","hub_lpc"]
  for mod in modules_base:
    b=bld(
      features = 'c cshlib',
      source = mod+'.c',
      target=mod,
      install_path = "${PREFIX}/lib/geepro/chips"
    ).env.cshlib_PATTERN       = '%s.chip'

